{
  "name": "remark-midas",
  "version": "5.0.0",
  "description": "Highlight CSS in Markdown files with midas.",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "LICENSE-MIT",
    "dist"
  ],
  "scripts": {
    "format": "remark *.md -qfo",
    "prepublishOnly": "npm run build",
    "build": "babel src --out-dir dist --ignore src/__tests__",
    "pretest": "eslint src",
    "test": "ava src/__tests__"
  },
  "keywords": [
    "css",
    "highlight",
    "syntax",
    "midas",
    "markdown",
    "remark"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "1.0.0-rc.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.0.0",
    "eslint-config-cssnano": "^3.0.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "remark": "^10.0.0",
    "remark-cli": "^6.0.0",
    "remark-html": "^8.0.0",
    "remark-preset-wooorm": "^4.0.0"
  },
  "homepage": "https://github.com/ben-eb/remark-midas",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/remark-midas",
  "dependencies": {
    "midas": "^2.0.0",
    "unist-util-visit": "^1.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "browserslist": "> 2.5%, node 6",
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "babel"
    ],
    "extends": "cssnano"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
